<editor>
	<class>ModelEditorDescriptor</class>
	<type>simple</type>
	<model>TextCategory</model>
	<changeTypes>
		<changeType>model-editor-TextCategory</changeType>
	</changeTypes>
	<title>
		<class>LanguageTitleME</class>
		<model>testLang</model>
		<field>title</field>
	</title>
	<data_fetchers>
		<data_fetcher>
			<class>SimpleDataFetcher</class>
		</data_fetcher>
		<data_fetcher>
			<class>LanguageDataFetcher</class>
		</data_fetcher>
	</data_fetchers>
	<hooks>
		<hook>
			<name>ModelEditor.form</name>
			<modules>
				<module>
					<name>FieldValidator</name>
					<instance>form</instance>
					<params>
						<param name="validator" type="simple">TextCategory</param>
					</params>
				</module>
				<module>
					<name>LanguageChooser</name>
					<instance>normal</instance>
				</module>
			</modules>
		</hook>
	</hooks>
	<inputs>
		<input>
			<key>title</key>
			<title>Title</title>
			<class>LanguageFieldME</class>
		</input>
	</inputs>
</editor>
