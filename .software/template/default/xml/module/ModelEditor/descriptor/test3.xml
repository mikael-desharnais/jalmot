<editor>
	<class>ModelEditorDescriptor</class>
	<type>simple</type>
	<model>test</model>
	<changeTypes>
		<changeType>model-editor-test</changeType>
	</changeTypes>
	<title>
		<class>LanguageTitleME</class>
		<model>testLang</model>
		<field>name</field>
	</title>
	<data_fetchers>
		<data_fetcher>
			<class>SimpleDataFetcher</class>
		</data_fetcher>
		<data_fetcher>
			<class>LanguageDataFetcher</class>
		</data_fetcher>
	</data_fetchers>
	<hooks>
		<hook>
			<name>ModelEditor.form</name>
			<modules>
				<module>
					<name>FieldValidator</name>
					<instance>form</instance>
					<params>
						<param name="validator" type="simple">test3</param>
					</params>
				</module>
				<module>
					<name>LanguageChooser</name>
					<instance>normal</instance>
				</module>
			</modules>
		</hook>
	</hooks>
	<inputs>
		<input>
			<key>valeur1</key>
			<title>Valeur 1</title>
			<class>SimpleRadioFieldME</class>
			<params>
				<param name="elements" type="array">
					<element key="0">Good</element>
					<element key="1">Pretty Good</element>
					<element key="2">Quite Perfect</element>
				</param>
			</params>
		</input>
		<input>
			<key>code</key>
			<title>Code</title>
			<class>SimpleFieldME</class>
		</input>
		<input>
			<key>name</key>
			<model>testLang</model>
			<title>Name</title>
			<class>LanguageFieldME</class>
		</input>
		<input>
			<key>description</key>
			<model>testLang</model>
			<title>Description</title>
			<class>HTMLLanguageFieldME</class>
		</input>
	</inputs>
</editor>
